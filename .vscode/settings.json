// 既定の設定を上書きするには、このファイル内に設定を挿入します
{
    "editor.fontFamily": "Consolas, Menlo, Monaco, 'Courier New', monospace",
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true,
        ".meta":true
    },
    "editor.cursorStyle": "line",
    "editor.cursorBlinking": "expand",
    "editor.renderIndentGuides": true
}